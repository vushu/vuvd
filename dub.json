{
    "authors": [
        "Dan Vu"
    ],
    "copyright": "Copyright Â© 2022, Dan Vu",
    "dependencies": {
        "bindbc-sdl": "~>1.2.1",
        "erupted": "~>2.1.69",
        "unit-threaded": "~>2.0.5"
    },
    "description": "A minimal D application.",
    "dflags-ldc": [
        "--output-bc",
        "-betterC"
    ],
    "libs": [
        "SDL2",
        "SDL2_image"
    ],
    "license": "proprietary",
    "name": "vuv",
    "subConfigurations": {
        "bindbc-sdl": "dynamicBC"
    },
    "versions": [
        "SDL_2010",
        "BindSDL_Image"
    ],
    "name": "vuvproject",
    "targetType": "executable",
    "targetPath": "bin",
    "configurations": [
        { "name": "executable" },
        {
            "name": "unittest",
            "targetType": "executable",
            "preBuildCommands": ["$DUB run --compiler=$$DC unit-threaded -c gen_ut_main -- -f bin/ut.d -d $DUB"],
            "mainSourceFile": "bin/ut.d",
            "excludedSourceFiles": ["source/app.d"],
            "dependencies": {
                "unit-threaded": "*"
            }
        }
    ]
}
